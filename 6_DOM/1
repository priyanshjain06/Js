const tempLiList = document.querySelectorAll('li') 
// get all list items as node list which is different from array

tempLiList.style.color="red" 
// gives error as tempLiList is not array
// to check this click on prototype and look for FOR EACH   which proves it not an array 

tempLiList[0].style.color="red"



tempLiList
tempLiList.foreach(function(item){
    item.style.color="white"
})


